   "metadata": {},
   "source": [
    "Syntax of If,elif & else\n",
    "\n",
    "if (Condition):\n",
    "   (Statement)\n",
    "elif(Condition):\n",
    "   (Statement)\n",
    "elif(Condition):\n",
    "   (Statement)\n",
    "else:\n",
    "   (Statement)"
   ]
  },
  {
   "cell_type": "raw",
   "id": "42e3a41c",
   "metadata": {},
   "source": [
    "Syntax of Nested If,elif & else\n",
    "\n",
    "if(Condition):\n",
    "    (Statement)\n",
    "    if(Condition):\n",
    "        (Statement)\n",
    "    else(Condition):\n",
    "        (Statement)\n",
    "elif(another condition):\n",
    "    if(Condition):\n",
    "        (Statement)\n",
    "    else(Condition):\n",
    "        (Statement)\n",
    "else:\n",
    "    (Statement)"
   ]
  },
